{
  "name": "highfive",
  "version": "1.0.0",
  "description": "HighFive peer recognition app",
  "main": "server.js",
  "engines": {
    "node": "6.3.0"
  },
  "scripts": {
    "build": "webpack -p --bail",
    "acceptance": "node \"node_modules/protractor/bin/protractor\" protractor.acceptance.conf.js",
    "client": "npm run delayed-open & wait",
    "coverage": "babel-node node_modules/babel-istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha ./test/index.jsx",
    "coverage-clean": "npm run rimraf -- test-artifacts/coverage",
    "coverage-open": "open coverage/index.html",
    "delayed-open": "sleep 5 && open http://localhost:2368",
    "deploy": "npm run build",
    "e2e": "node \"node_modules/protractor/bin/protractor\" protractor.conf.js",
    "e2e-test-server": "concurrently --kill-others \"npm run server\" \"webpack-dev-server \"",
    "lint": "eslint src --ext .jsx,.js",
    "lint-ci": "eslint src --ext .jsx,.js -o ./test-artifacts/junit.eslint.xml -f junit ; exit 0",
    "postacceptance": "node \"node_modules/babel-istanbul/lib/cli.js\" report --include=coverage/*.json && open coverage/index.html",
    "poste2e": "node \"node_modules/babel-istanbul/lib/cli.js\" report --include=coverage/*.json && open coverage/index.html",
    "posttest": "npm run lint-ci && npm run coverage",
    "posttest-ci": "npm run coverage && npm run lint-ci",
    "preacceptance": "npm run coverage-clean",
    "precoverage": "npm run coverage-clean",
    "prebuild": "npm prune && npm install && npm run clean",
    "pree2e": "node \"node_modules/protractor/bin/webdriver-manager\" update && npm run coverage-clean",
    "preinstall": "npm link test/helpers/ajax-helper || cp -r test/helpers/ajax-helper node_modules",
    "pretest": "npm run clean",
    "clean": "npm run rimraf -- dist test-artifacts",
    "rimraf": "node \"node_modules/rimraf/bin\"",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "\"node_modules/.bin/nodemon\" --watch package.json --watch webpack.config.js --watch server.js server.js",
    "test": "mocha ./test/index.jsx --compilers js:babel-core/register",
    "test-ci": "mocha ./test/index.jsx --compilers js:babel-core/register --reporter mocha-junit-reporter --reporter-options mochaFile=./test-artifacts/junit.mocha.xml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arielpartners/highfive-client.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "json-server": "0.8.14",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "express-http-proxy": "^0.6.0",
    "serve-static": "^1.11.1"
  },
  "devDependencies": {
    "archiver": "1.0.1",
    "autoprefixer": "6.3.6",
    "babel-cli": "6.11.4",
    "babel-core": "6.9.1",
    "babel-eslint": "6.1.2",
    "babel-istanbul": "0.11.0",
    "babel-loader": "6.2.4",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.11.5",
    "babel-plugin-transform-function-bind": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.11.6",
    "babel-runtime": "6.6.1",
    "blob": "0.0.4",
    "bootstrap-sass": "^3.3.7",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chart.js": "^1.1.1",
    "classnames": "2.2.5",
    "concurrently": "2.1.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "cucumber": "1.2.0",
    "deep-freeze": "0.0.1",
    "enzyme": "2.3.0",
    "eslint": "2.11.1",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "5.1.1",
    "expect.js": "0.3.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fake-xml-http-request": "1.4.0",
    "file-loader": "0.8.5",
    "gitinfo": "1.2.2",
    "griddle-react": "0.6.1",
    "html-webpack-plugin": "2.20.0",
    "install": "0.8.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "jsdom": "9.2.1",
    "json-loader": "0.5.4",
    "material-design-icons": "2.2.3",
    "material-design-lite": "1.1.3",
    "minimatch": "3.0.2",
    "mocha": "2.5.3",
    "mocha-junit-reporter": "^1.12.0",
    "mock-raf": "0.0.2",
    "moment": "2.14.1",
    "node-libs-browser": "0.5.2",
    "node-sass": "3.7.0",
    "nodemon": "^1.10.2",
    "npm": "3.9.5",
    "opener": "1.4.1",
    "postcss-loader": "^0.11.0",
    "promise": "7.1.1",
    "protractor": "3.3.0",
    "protractor-cucumber-framework": "0.6.0",
    "protractor-istanbul-plugin": "2.0.0",
    "proxyquire": "1.7.9",
    "react": "15.1.0",
    "react-addons": "0.9.1-deprecated",
    "react-addons-test-utils": "15.1.0",
    "react-chartjs": "^0.8.0",
    "react-dom": "15.0.2",
    "react-hot-loader": "1.2.7",
    "react-redux": "4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-stub-context": "0.7.0",
    "recursive-readdir": "2.0.0",
    "redux": "3.5.2",
    "redux-mock-store": "1.1.0",
    "redux-observable": "0.9.0",
    "redux-persist": "3.2.2",
    "rimraf": "2.5.2",
    "rxjs": "5.0.0-beta.11",
    "sass-loader": "3.2.0",
    "semver": "5.1.1",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.10.0",
    "webpack-load-plugins": "0.1.2",
    "yargs": "4.7.1"
  }
}
